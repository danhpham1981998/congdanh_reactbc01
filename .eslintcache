utton className=\"btn btn-primary mr-1\" onClick={() => {this.props.thayDoiSoLuong(spGioHang.maSP,1)}}>+</button>\r\n                                        {spGioHang.soLuong}\r\n                                        <button className=\"btn btn-primary ml-2\" onClick={() => {this.props.thayDoiSoLuong(spGioHang.maSP,-1)}}>-</button>\r\n                                    </td>\r\n                                    <td>{spGioHang.giaBan}</td>\r\n                                    <td>{spGioHang.giaBan * spGioHang.soLuong}</td>\r\n                                    <td>\r\n                                        <button className=\"btn btn-danger\" onClick={() => {this.props.xoaGioHang(spGioHang.maSP)}}>Xóa</button>\r\n                                    </td>\r\n                                </tr>\r\n                            })}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\n//Hàm chuyển state trên redux trờ thành props của component\r\n//State đại diện cho rootReducer\r\nconst mapStateToProps = (state) => {\r\n\r\n    //Tạo props từ state redux\r\n    return {\r\n        gioHang: state.gioHangReducer.gioHang\r\n    }\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n    return {\r\n        thayDoiSoLuong: (maSP,soLuong) => {\r\n            console.log('thaydoigiohang', maSP)\r\n            //Tạo action\r\n            const action = {\r\n                type: 'THAY_DOI_SO_LUONG',\r\n                maSP:maSP,\r\n                soLuong:soLuong\r\n            }\r\n            //Đưa dữ liệu lên reducer\r\n            dispatch(action);\r\n        },\r\n        xoaGioHang: (maSP) => {\r\n            console.log('xóa nè', maSP);\r\n            //Tạo action\r\n            const action = {\r\n                type: 'XOA_GIO_HANG',\r\n                maSP:maSP\r\n            }\r\n            //Đưa dữ liệu lên reducer\r\n            dispatch(action);\r\n        }\r\n    }\r\n}\r\n\r\n//Kết nối giữa component và redux\r\n//Biến component này có chứa store redux ( nâng cao)\r\nexport default connect(mapStateToProps, mapDispatchToProps)(GioHangRedux);","C:\\Users\\Administrator\\Desktop\\reactbc01\\reactbc01\\src\\DemoRedux\\BTGioHangRedux.js\\SanPhamRedux.js",[],{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","severity":1,"message":"42","line":1,"column":8,"nodeType":"43","messageId":"44","endLine":1,"endColumn":12},{"ruleId":"41","severity":1,"message":"45","line":4,"column":10,"nodeType":"43","messageId":"44","endLine":4,"endColumn":24},{"ruleId":"41","severity":1,"message":"46","line":5,"column":8,"nodeType":"43","messageId":"44","endLine":5,"endColumn":20},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'gioHangReducer' is defined but never used.","'SanPhamRedux' is defined but never used.","no-global-assign","no-unsafe-negation"]